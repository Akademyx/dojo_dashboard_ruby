<div id="wrapper">

    <h1>Listing 4 Dojos</h1>
    
    
    <table>
        <tr>
            <th>Coding Dojo Branch</th>
            <th>Street</th>
            <th>City</th>
            <th>State</th>
            <th>Action</th>
        </tr>
        <% @loc.each do |p| %>
        <tr>
            <td><%= p['branch'] %></td>
            <td><%= p['street'] %></td>
            <td><%= p['city'] %></td>
            <td><%= p['state'] %></td>
            <td><a href="/dojos/<%=p['id']%>">Show</a> | <a href="/dojos/<%=p['id']%>/edit">Edit</a> | <a href="/dojos/<%=p['id']%>">Destroy</a></td>
        </tr>
        <% end %>
    </table>
    <a href="/dojos/new">New Dojo</a>
</div>