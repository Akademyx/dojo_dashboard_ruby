<h1><%= @dojo.branch%>'s <%= @student.first_name %> <%= @student.last_name %></h1>


<h4><%= @student.email %></h4>

<h2>Cohort</h2>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% @others.each do |o| %>
            <tr>
                <td><%= o.first_name %></td>
                <td><%= o.last_name %></td>
                <td><%= o.email %></td>
            </tr>
            <% end %>
    </tbody>
</table>
<p>
    <a href="/dojos">Home</a>
    <a href="/dojos/<%= @dojo.id %>/students/<%= @student.id %>/edit">Edit</a>
</p>